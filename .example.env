# FastWrap Environment Variables
# Copy this file to .env and fill in the required values

# --- Service ---
FASTWRAP_API_KEY=1234
PORT=8555
HOST=0.0.0.0

# --- Redis ---
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_API_KEY=
REDIS_USER=User123
REDIS_USER_PW=

# --- PostgreSQL ---
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=fastwrap_db
POSTGRES_USER=postgres
POSTGRES_PW=postgres
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/fastwrap_db

# --- LLM ---
MODEL=gpt-4o-mini
MODEL_KEY=
# MODEL_PROVIDER=  # Optional: set if auto-detection fails

# --- Embeddings ---
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIM=1536

# --- LangSmith (optional) ---
# LANGCHAIN_API_KEY=
# LANGSMITH_TRACING_V2=true
